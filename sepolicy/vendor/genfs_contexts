# Battery
genfscon sysfs /class/qcom-battery                                                  u:object_r:vendor_sysfs_battery_supply:s0
genfscon sysfs /class/qcom-battery/reverse_chg_mode                                 u:object_r:sysfs_powershare:s0

# Fingerprint
genfscon sysfs /devices/virtual/touch/touch_dev/fod_press_status                    u:object_r:sysfs_tp_fodstatus:s0